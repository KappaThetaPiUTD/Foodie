# Google Maps API Key (same as NEXT_PUBLIC_GOOGLE_MAPS_API_KEY)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=copy_from_web_env

# Google OAuth Client ID (same as NEXT_PUBLIC_GOOGLE_CLIENT_ID)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=copy_from_web_env

# Firebase Configuration (copy values from NEXT_PUBLIC_FIREBASE_* in apps/web/.env.local)
EXPO_PUBLIC_FIREBASE_API_KEY=copy_from_web_env
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=copy_from_web_env
EXPO_PUBLIC_FIREBASE_PROJECT_ID=copy_from_web_env
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=copy_from_web_env
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=copy_from_web_env
EXPO_PUBLIC_FIREBASE_APP_ID=copy_from_web_env
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=copy_from_web_env

# Backend Socket.IO URL (use your computer's IP, not localhost)
EXPO_PUBLIC_SOCKET_URL=http://YOUR_LOCAL_IP:5000
